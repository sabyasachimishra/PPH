<?xml version="1.0" encoding="UTF-8"?>
<web-bnd 
	xmlns="http://websphere.ibm.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://websphere.ibm.com/xml/ns/javaee http://websphere.ibm.com/xml/ns/javaee/ibm-web-bnd_1_0.xsd"
	version="1.0">

	<virtual-host name="default_host" />

	<resource-ref name="LOGINDB" binding-name="jdbc/LOGINDB">
		<authentication-alias name="LOGINDB" />
	</resource-ref>
	<resource-ref name="PAT_DB" binding-name="jdbc/PAT_DB">
		<authentication-alias name="PAT_DB" />
	</resource-ref>
	<resource-ref name="EMP_DB" binding-name="jdbc/EMP_DB">
		<authentication-alias name="EMP_DB" />
	</resource-ref>
</web-bnd>
